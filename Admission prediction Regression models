   "cell_type": "code",
   "execution_count": 20,
   "id": "2c4df7a3",
   "metadata": {},
   "outputs": [],
   "source": [
    "## Adjusted R2_Score\n",
    "\n",
    "def adjr2_score(x,y):\n",
    "    sc=linear.score(x,y)\n",
    "    n=x.shape[0]\n",
    "    p=x.shape[1]\n",
    "    adj = 1-(1-sc)*(n-1)/(n-p-1)\n",
    "    return adj"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "id": "2c9b856c",
   "metadata": {},
